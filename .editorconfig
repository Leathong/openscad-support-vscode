
# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,ts}]
charset = utf-8

# Tab indentation
[*.{js,ts}]
indent_style = tab
quote_type = single
tab_width = 4
